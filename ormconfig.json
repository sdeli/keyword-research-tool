{
  "type": "postgres",
  "synchronize": true,
  "logging": false,
  "seeds": [
    "dist/seeds/*.seed.js"
  ],
  "factories": [
    "dist/seeds/*.factory.js"
  ],
  "entities": [
    "dist/**/*.entity.js"
  ],
  "migrations": [
    "dist/migrations/**/*.js"
  ],
  "cli": {
    "entitiesDir": "src/entity",
    "migrationsDir": "src/migrations"
  }
}